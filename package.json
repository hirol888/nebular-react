    "yargs": "^17.0.1"
  },
  "dependencies": {
    "@emotion/react": "11.9.3",
    "@emotion/server": "11.4.0",
    "@emotion/styled": "^11.10.0",
    "@mantine/hooks": "^6.0.1",
    "@mantine/utils": "^6.0.1",
    "clsx": "1.1.1",
    "csstype": "3.0.9",
    "eva-icons": "^1.1.3",
    "html-react-parser": "1.4.12",
    "klona": "^2.0.5",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prism-react-renderer": "^1.2.1",
    "react": "18.1.0",
    "react-transition-group": "4.4.2",
    "zod": "^3.13.4"
  }
}
