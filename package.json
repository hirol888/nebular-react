    "@mantine/core": "^8",
    "@mantine/dates": "^8",
    "@mantine/form": "^8",
    "@mantine/hooks": "^8",
    "@mantine/utils": "^6.0.22",
    "clsx": "2.1.1",
    "csstype": "3.1.3",
    "eslint-config-next": "13",
    "eva-icons": "^1.1.3",
    "html-react-parser": "5.2.5",
    "klona": "^2.0.6",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.0",
    "react-transition-group": "4.4.5",
    "zod": "^3.25.32"
  }
}
